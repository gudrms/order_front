# Supabase Database URLs
# Supabase 프로젝트 설정 > Database > Connection String에서 확인
DATABASE_URL="postgresql://postgres:[YOUR-PASSWORD]@db.[YOUR-PROJECT-REF].supabase.co:5432/postgres?pgbouncer=true"
DIRECT_URL="postgresql://postgres:[YOUR-PASSWORD]@db.[YOUR-PROJECT-REF].supabase.co:5432/postgres"

# Supabase API Keys
# Supabase 프로젝트 설정 > API > Project API keys에서 확인
SUPABASE_URL="https://[YOUR-PROJECT-REF].supabase.co"
SUPABASE_ANON_KEY="eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9..."
SUPABASE_SERVICE_KEY="eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9..."

# OKPOS API (아직 미사용)
OKPOS_API_KEY=""
OKPOS_BASE_URL="https://dum.okpos.co.kr/api"

# Node Environment
NODE_ENV=development

# CORS 설정 (보안)
# 프로덕션: Frontend URL 설정 필요
# 개발: localhost 자동 허용
FRONTEND_URL="https://order-front-frontend.vercel.app"

# Sentry 에러 모니터링
# Sentry 프로젝트 설정에서 DSN 복사
SENTRY_DSN=https://examplePublicKey@o0.ingest.sentry.io/0
SENTRY_ENVIRONMENT=development
