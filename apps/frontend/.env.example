# ============================================
# 환경 변수 예시 파일
# ============================================
# 이 파일을 복사하여 .env.local 또는 .env.development.local 등을 생성하세요.

# 앱 기본 정보
NEXT_PUBLIC_APP_NAME=Table Order System
NEXT_PUBLIC_APP_VERSION=0.1.0

# API 서버 설정
NEXT_PUBLIC_API_URL=http://localhost:3000/api/v1
# NEXT_PUBLIC_WS_URL=wss://api.example.com/ws-stomp

# Mock 데이터 설정
NEXT_PUBLIC_USE_MOCK=true
NEXT_PUBLIC_STORE_ID=store-default-1

# 기본 매장/지점 설정
NEXT_PUBLIC_DEFAULT_STORE_TYPE=tacomolly
NEXT_PUBLIC_DEFAULT_BRANCH_ID=gimpo

# 개발 도구
NEXT_PUBLIC_ENABLE_QUERY_DEVTOOLS=true
NEXT_PUBLIC_LOG_LEVEL=debug

# Supabase (선택 사항)
# NEXT_PUBLIC_SUPABASE_URL=
# NEXT_PUBLIC_SUPABASE_ANON_KEY=
