# API 서버 설정
NEXT_PUBLIC_API_URL=https://order-front-backend.vercel.app/api/v1
NEXT_PUBLIC_WS_URL=wss://order-front-backend.vercel.app/ws-stomp

# 매장 설정
NEXT_PUBLIC_STORE_ID=1
NEXT_PUBLIC_USE_MOCK=false

# 기본 매장/지점 설정
NEXT_PUBLIC_DEFAULT_STORE_TYPE=tacomolly
NEXT_PUBLIC_DEFAULT_BRANCH_ID=gimpo

# 프로덕션 최적화
NEXT_PUBLIC_ENABLE_QUERY_DEVTOOLS=false
NEXT_PUBLIC_LOG_LEVEL=error
